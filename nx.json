{
  "npmScope": "poc-nx-firebase",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "base-app": {
      "tags": []
    },
    "base-app-e2e": {
      "tags": [],
      "implicitDependencies": ["base-app"]
    },
    "fire-app": {
      "tags": []
    },
    "foobar": {
      "tags": []
    },
    "react-xtend": {
      "tags": []
    },
    "react-xtend-e2e": {
      "tags": [],
      "implicitDependencies": ["react-xtend"]
    },
    "types-types": {
      "tags": ["types"]
    }
  }
}
